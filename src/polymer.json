{
    "entrypoint": "index.html",
    "shell": "elements/my-app.html",
    "fragments": [
        "elements/my-view-404.html",
        "elements/my-view-about.html",
        "elements/my-view-impress.html",
        "elements/my-view-posts.html",
        "elements/my-view-projects.html"
    ],
    "sources": [
        "elements/**/*",
        "images/**/*",
        "api/**/*",
        "feeds/**/*"
    ],
    "extraDependencies": [
        "bower_components/webcomponentsjs/*.js"
    ],
    "lint": {
        "rules": ["polymer-2"],
        "ignoreWarnings": ["undefined-elements"]
    },
    "builds": [{
        "addServiceWorker": true,
        "bundle": true,
        "js": {"minify": true},
        "css": {"minify": true},
        "html": {"minify": true}
    }]
}
