{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "@11ty/eleventy-plugin-rss": "1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.6",
    "chokidar": "3.5.0",
    "clean-css": "4.2.3",
    "concurrently": "5.3.0",
    "html-minifier": "4.0.0",
    "markdown-it-anchor": "7.0.0",
    "node-fetch": "2.6.1",
    "p-limit": "3.1.0",
    "posthtml": "0.15.1",
    "prettier": "2.2.1"
  },
  "scripts": {
    "start": "concurrently -n css,site \"node lib/build-css.js --watch\" \"eleventy --serve\"",
    "build": "node lib/build-css.js && eleventy",
    "format": "prettier --write \"**/*.{html,js,css,md,njk}\"",
    "update-projects": "node lib/update-projects.js"
  }
}
