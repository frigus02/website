{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@11ty/eleventy-plugin-rss": "1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.1.2",
    "chokidar": "3.5.2",
    "clean-css": "5.2.0",
    "concurrently": "6.3.0",
    "html-minifier": "4.0.0",
    "markdown-it-anchor": "8.3.0",
    "node-fetch": "3.0.0",
    "p-limit": "4.0.0",
    "posthtml": "0.16.5",
    "prettier": "2.4.1"
  },
  "scripts": {
    "start": "node lib/update-projects.mjs && concurrently -n css,site \"node lib/build-css.mjs --watch\" \"eleventy --serve\"",
    "build": "NODE_ENV=production yarn _build_internal",
    "_build_internal": "node lib/update-projects.mjs && node lib/build-css.mjs && eleventy",
    "format": "prettier --write \"**/*.{html,js,css,md,njk}\""
  }
}
