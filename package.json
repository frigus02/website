{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "1.0.2",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.2.0",
    "chokidar": "3.5.3",
    "clean-css": "5.3.2",
    "concurrently": "7.6.0",
    "html-minifier": "4.0.0",
    "markdown-it-anchor": "8.6.6",
    "node-fetch": "3.3.0",
    "p-limit": "4.0.0",
    "posthtml": "0.16.6",
    "prettier": "2.8.4"
  },
  "scripts": {
    "start": "node lib/update-projects.mjs && concurrently -n css,site \"node lib/build-css.mjs --watch\" \"eleventy --serve\"",
    "build": "NODE_ENV=production yarn _build_internal",
    "_build_internal": "node lib/update-projects.mjs && node lib/build-css.mjs && eleventy",
    "format": "prettier --write \"**/*.{html,js,css,md,njk}\""
  }
}
